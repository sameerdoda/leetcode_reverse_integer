    "def reverse(x):\n",
    "        num = 1\n",
    "        if x < 0:\n",
    "            num = -1\n",
    "        x = int(str(abs(x))[::-1])*num\n",
    "        if x in range(-(2**31), 2**31):\n",
    "            return x\n",
    "        else:\n",
    "            return 0"
   ]
